{
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[mdx]": {
    "editor.wordWrap": "on",
    "editor.wrappingIndent": "same"
  },
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "cssModules.camelCase": false,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.tabSize": 2,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "emmet.syntaxProfiles": {
    "html": {
      "attr_quotes": "single"
    },
    "javascript": {
      "self_closing_tag": true
    }
  },
  "emmet.triggerExpansionOnTab": true,
  "git.autofetch": true,
  "prettier.requireConfig": true,
  "jest.coverageColors": {
    "covered": "rgba(9, 156, 65, 1)",
    "uncovered": "rgba(0, 0, 0, 1)",
    "partially-covered": "rgba(235, 198, 52, 1)"
  },
  "jest.jestCommandLine": "yarn test",
  "jest.showCoverageOnLoad": true,
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#600",
    "activityBar.activeBorder": "#a99",
    "activityBar.background": "#800",
    "activityBar.foreground": "#a99",
    "activityBar.inactiveForeground": "#500",
    "activityBarBadge.background": "#a99",
    "activityBarBadge.foreground": "#500"
  }
}
